{
    "name": "naif/ai-validation",
    "description": "AI Validator for Laravel: Uses ChatGPT (GPT-3.5-turbo) to easily check and filter user inputs.",
    "type": "library",
    "require": {
        "php": "^7.2|^8.0",
        "illuminate/support": "^6.0|^8.0|^9.0|^10.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Naif\\AIValidation\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Naif Alshaye",
            "email": "naif@naif.io"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Naif\\AIValidation\\AIValidationServiceProvider"
            ]
        }
    }
}
