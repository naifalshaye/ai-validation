{
    "name": "naif/spam-validation-rule",
    "description": "An efficient Laravel validation rule using ChatGPT to identify and filter spam input.",
    "type": "library",
    "require": {
        "php": "^8.1",
        "illuminate/support": "^8.0|^9.0",
        "openai-php/client": "^0.8.1",
        "symfony/http-client": "^6.4",
        "nyholm/psr7": "^1.8"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Naif\\SpamValidationRule\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Naif Alshaye",
            "email": "naif@naif.io"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Naif\\SpamValidationRule\\SpamValidationRuleServiceProvider"
            ]
        }
    }
}
